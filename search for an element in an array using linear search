import java.io.*;
import java.lang.*;

class GFG {
    
    public static int fun(int[] a, int x)
    {
      for(int i = 0;i<a.length;i++)
      {
        if(a[i]==x)
        {
           return i;
        }
      }
      return -1;
    }
	public static void main (String[] args) {
	    int a[]={7,9,8,2,5,3};
	    int x = 5;
	
	   System.out.println(fun(a,x));
	}
}
