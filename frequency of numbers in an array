import java.io.*;
import java.lang.*;

class GFG {
    
    public static int fun(int[] a, int x)
    {
        int count=0;
      for(int i = 0;i<a.length;i++)
      {
        if(a[i]==x)
        {
           count++;
        }
      }
      return count;
    }
	public static void main (String[] args) {
	    int a[]={7,9,8,2,5,3,3,3};
	    int x = 3;
	
	   System.out.println(fun(a,x));
	}
}
