
class Compute {
    
    public void rotate(int a[], int n)
    {
      int i=0, j=n-1,temp;
      while(true)
      {
          if(i<j)
          {
          temp=a[i];
          a[i]=a[j];
          a[j]=temp;
          i++;
          }
          else{
              break;
          }
          
    }
   
}
}
