import java.io.*;
import java.lang.*;

public class Gfg{
    public static void main(String[] args)
    {
      int n=543;
      int r, sum = 0 , temp;
      temp = n;
      while(n>0)
      {
         r = n%10;
         sum = sum*10+r;
         n = n/10;
      }
      n = temp;
      if(n==sum)
      {
          System.out.println("It is a Plaindrome");
      }
      else{
          System.out.println("Not a Palindrome");
      }
    }
}
